    "@types/react-redux": "^7.1.0",
    "@types/recompose": "^0.30.6",
    "@types/webaudioapi": "^0.0.27",
    "@types/webpack": "^4.4.33",
    "@types/webpack-bundle-analyzer": "^2.13.1",
