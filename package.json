    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "color": "^3.1.0",
    "cuid": "^2.1.4",
    "emotion": "^9.2.12",
    "immer": "^1.7.4",
    "prop-types": "^15.6.2",
    "raf-throttle": "^2.0.3",
    "ramda": "^0.25.0",
