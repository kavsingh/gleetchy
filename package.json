  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "TS_NODE_PROJECT='./tsconfig.ts-node.json' webpack serve --env development",
    "dev:public": "PUBLIC=true TS_NODE_PROJECT='./tsconfig.ts-node.json' webpack serve --env development",
    "lint:ts": "eslint .",
    "lint:css": "stylelint '**/*.ts' '**/*.tsx'",
    "lint": "yarn lint:ts && yarn lint:css",
    "jest": "NODE_ENV=test jest",
    "test": "tsc && npm run jest -- --coverage --verbose",
    "test:mutation": "stryker run",
    "tdd": "yarn jest --watch",
    "bundle:inspect": "BUNDLE_ANALYZE=true yarn build",
    "prebuild": "rimraf dist",
    "build": "TS_NODE_PROJECT='./tsconfig.ts-node.json' webpack --env production",
    "prestatic": "rimraf dist-static",
    "static": "./ts scripts/render-static",
    "export": "yarn build && yarn static",
    "servestatic": "cd dist-static && ws --spa"
  },
