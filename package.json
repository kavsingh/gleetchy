{
  "name": "gleetchy",
  "version": "1.0.0",
  "description": "looping business",
  "author": "Kav Singh <flakisms@gmail.com> (https://github.com/kavsingh)",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/kavsingh/gleetchy.git",
    "type": "git"
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "start:public": "PUBLIC=true NODE_ENV=development webpack-dev-server",
    "lint": "tslint 'src/**/*.ts?(x)' && eslint .",
    "jest": "NODE_ENV=test jest --config=.jestrc.js",
    "test": "npm run jest -- --coverage --verbose",
    "test:watch": "npm run jest -- --watch",
    "generate:instrument": "node ./scripts/bin/generateNode.js instrument",
    "generate:audioEffect": "node ./scripts/bin/generateNode.js audioEffect",
    "generate:component": "node ./scripts/bin/generateComponent.js",
    "generate:state": "node ./scripts/bin/generateState.js",
    "bundle:inspect": "BUNDLE_ANALYZE=true yarn build",
    "bundle:visualize": "BUNDLE_VISUALIZE=true yarn build && open stats.html",
    "build": "rimraf dist && NODE_ENV=production webpack --progress --colors",
    "static": "rimraf distStatic && NODE_ENV=production node renderStatic",
    "export": "npm run build && npm run static"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
