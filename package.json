{
  "name": "gleetchy",
  "version": "1.0.0",
  "description": "looping business",
  "author": "Kav Singh <flakisms@gmail.com> (https://github.com/kavsingh)",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/kavsingh/gleetchy.git",
    "type": "git"
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "start:public": "PUBLIC=true NODE_ENV=development webpack-dev-server",
    "build": "rimraf dist && NODE_ENV=production webpack",
    "lint": "eslint .",
    "test": "NODE_ENV=test jest --coverage --verbose",
    "test:watch": "NODE_ENV=test jest --watch",
    "generate:instrument": "node ./scripts/bin/generateNode.js instrument",
    "generate:audioEffect": "node ./scripts/bin/generateNode.js audioEffect",
    "generate:component": "node ./scripts/bin/generateComponent.js",
    "generate:state": "node ./scripts/bin/generateState.js",
    "inspect": "BUNDLE_ANALYZE=true yarn build",
    "visualize": "BUNDLE_VISUALIZE=true yarn build && open stats.html"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.32",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.32",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.32",
    "@babel/polyfill": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-react": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "@babel/runtime": "^7.0.0-beta.32",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.3.0-beta.8",
    "babel-loader": "^8.0.0-beta.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "chalk": "^2.3.0",
    "common-tags": "^1.5.1",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-mongrel-react":
      "kavsingh/eslint-config-mongrel-react.git#5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.3.0-beta.8",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-pwa-manifest": "^3.3.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "color": "^2.0.1",
    "popmotion": "^7.8.2",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-favicon": "^0.0.11",
    "react-icons": "^2.2.7",
    "react-input-autosize": "^2.1.2",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-jsx": "^2.2.0"
  }
}
