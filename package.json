{
  "name": "gleetchy",
  "version": "1.0.0",
  "description": "looping business",
  "author": "Kav Singh <flakisms@gmail.com> (https://github.com/kavsingh)",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/kavsingh/gleetchy.git",
    "type": "git"
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack-dev-server",
    "dev:public": "PUBLIC=true NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack-dev-server",
    "lint:ts": "tsc && eslint --ext .js,.ts,.tsx .",
    "lint:css": "stylelint '**/*.ts' '**/*.tsx' '**/*.js' '**/*.jsx'",
    "lint": "npm run lint:ts && npm run lint:css",
    "jest": "NODE_ENV=test jest",
    "test": "tsc && npm run jest -- --coverage --verbose",
    "test:mutation": "stryker run",
    "tdd": "npm run jest -- --watch",
    "bundle:inspect": "BUNDLE_ANALYZE=true yarn build",
    "build": "rimraf dist && NODE_ENV=production TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack --progress --colors",
    "static": "rimraf dist-static && NODE_ENV=production TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/render-static",
    "export": "npm run build && npm run static",
    "servestatic": "cd dist-static && ws --spa"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
