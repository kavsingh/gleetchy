    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-emotion": "^10.0.5",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-webpack-alias": "^2.1.2",
