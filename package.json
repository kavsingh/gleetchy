{
  "name": "gleetchy",
  "version": "1.0.0",
  "description": "looping business",
  "author": "Kav Singh <flakisms@gmail.com> (https://github.com/kavsingh)",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/kavsingh/gleetchy.git",
    "type": "git"
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "start:public": "PUBLIC=true NODE_ENV=development webpack-dev-server",
    "lint": "eslint .",
    "jest": "NODE_ENV=test jest --config=.jestrc.js",
    "test": "npm run jest -- --coverage --verbose",
    "test:watch": "npm run jest -- --watch",
    "generate:instrument": "node ./scripts/bin/generateNode.js instrument",
    "generate:audioEffect": "node ./scripts/bin/generateNode.js audioEffect",
    "generate:component": "node ./scripts/bin/generateComponent.js",
    "generate:state": "node ./scripts/bin/generateState.js",
    "bundle:inspect": "BUNDLE_ANALYZE=true yarn build",
    "bundle:visualize": "BUNDLE_VISUALIZE=true yarn build && open stats.html",
    "build": "rimraf dist && NODE_ENV=production webpack",
    "static": "rimraf distStatic && NODE_ENV=production node renderStatic",
    "export": "npm run build && npm run static"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.39",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.39",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0-beta.39",
    "@babel/runtime": "^7.0.0-beta.39",
    "babel-7-jest": "^21.3.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-emotion": "^9.0.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-webpack-alias": "^2.1.2",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "common-tags": "^1.7.2",
    "emotion-server": "9.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-mongrel-react": "kavsingh/eslint-config-mongrel-react.git#5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.2",
    "preact-render-to-string": "^3.7.0",
    "prettier": "^1.10.2",
    "raf": "^3.4.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-pwa-manifest": "^3.4.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "color": "^3.0.0",
    "emotion": "^9.0.0",
    "popmotion": "^8.1.10",
    "popmotion-react": "^2.1.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-favicon": "^0.0.13",
    "react-icons": "^2.2.7",
    "react-input-autosize": "^2.2.1",
    "react-redux": "^5.0.6",
    "react-style-proptype": "^3.2.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  }
}
